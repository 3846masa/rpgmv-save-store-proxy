{
  "env": {
    "server": {
      "presets": [
        "es2015-node5"
      ]
    },
    "client": {
      "presets": [
        "es2015"
      ]
    }
  },
  "plugins": [
    "syntax-async-functions",
    [
      "transform-async-to-module-method",
      {
        "module": "co",
        "method": "wrap"
      }
    ]
  ]
}
